    "@types/react-dom": "16.0.11",
    "@types/react-router": "4.0.25",
    "@types/react-router-dom": "4.2.6",
    "@types/react-test-renderer": "16.0.1",
    "@types/reactstrap": "^8.7.2",
    "@types/recharts": "^1.8.23",
    "@types/text-encoding": "0.0.35",
