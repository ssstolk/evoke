{
  "name": "evoke",
  "version": "1.4.1",
  "description": "Evoke",
  "author": "Sander Stolk",
  "license": "GPL-3.0",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --env prod=true",
    "clean": "rmdir /s /q node_modules",
    "postbuild": "npm run build-war",
    "build-dev": "webpack",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage-remap && npm run coverage-report-html",
    "coverage-remap": "remap-istanbul -i reports/coverage/coverage.json -o reports/coverage/coverage.json -t json",
    "coverage-report-html": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage html",
    "precommit": "lint-staged",
    "autoformat": "prettier --write \"**/*.{ts,tsx,js,json,css,less}\"",
    "tslint": "tslint --project . --format verbose",
    "tslint-fix": "tslint --project . --format verbose --fix",
    "build-war": "node tools/make-war.js"
  },
  "dependencies": {
    "@comunica/actor-init-sparql-rdfjs": "^1.22.3",
    "asynciterator": "^3.2.1",
    "buffer": "^6.0.3",
    "classnames": "2.2.5",
    "core-js": "2.5.6",
    "d3": "^5.16.0",
    "d3pie": "^0.2.1",
    "isomorphic-fetch": "2.2.1",
    "jquery": "^3.6.0",
    "jquery.longpress": "file:node_modules_custom/jquery-longpress",
    "jsonld-streaming-parser": "^2.4.2",
    "n3": "^1.11.2",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-dom": "16.6.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "reactstrap": "^8.10.1",
    "recharts": "^1.8.5",
    "stream-browserify": "^3.0.0",
    "text-encoding": "^0.7.0",
    "trashable": "^1.0.7",
    "tslib": "^2.3.1",
    "tv4": "1.3.0",
    "twitter-text": "^3.1.0",
    "uuid": "^3.4.0",
    "wordcloud": "^1.2.2"
  },
  "devDependencies": {
    "@types/asynciterator": "^1.1.4",
    "@types/chai": "4.1.3",
    "@types/chai-as-promised": "7.1.0",
